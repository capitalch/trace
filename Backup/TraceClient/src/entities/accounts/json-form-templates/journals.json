                    "validations": [
                        {
                            "name": "required",
                            "message": "This is required"
                        }
                    ]
                },
                {
                    "type": "Text",
                    "name": "userRefNo",
                    "placeholder": "User ref",
                    "label": "User ref"
                },
                {
                    "type": "Text",
                    "name": "tags",
                    "placeholder": "Comma separated tags",
                    "label": "Search tags",
                    "class": "x-tags"
                },
                {
                    "type": "Textarea",
                    "name": "remarks",
                    "class": "x-header-remarks",
                    "placeholder": "Common remarks",
                    "label": "Common remarks"
                }
            ]
        },
        {
            "type": "Set",
            "name": "totals",
            "class": "x-display-journal-totals",
            "items": [
                {
                    "type": "VoucherTotals",
                    "name": "debits1",
                    "class": "decimal-right x-debit-totals",
                    "ibukiFilterOnMessage": "DEBIT-CHANGED",
                    "value": "",
                    "dc": "D"
                },
                {
                    "type": "VoucherTotals",
                    "name": "credits1",
                    "class": "decimal-right",
                    "ibukiFilterOnMessage": "CREDIT-CHANGED",
                    "value": "",
                    "dc": "C"
                }

            ]
        },
        {
            "type": "Range",
            "name": "debits",
            "label": "Debits",
            "layout": "table",
            "pattern": {
                "type": "Set",
                "name": "debit",
                "class": "x-range-set",
                "validations": [
                    {
                        "name": "gstPaymentVoucherValidation",
                        "message": "GST calculation error"
                    }
                ],
                "items": [
                    {
                        "type": "LedgerSubledger",
                        "name": "accId",
                        "placeholder": "Debit account",
                        "label": "Debit account",
                        "validations": [
                            {
                                "name": "required",
                                "message": "Ledger / subleder is required"
                            }
                        ],
                        "options": {
                            "args": [
                                "branch"
                                , "capital"
                                , "other"
                                , "loan"
                                , "iexp"
                                , "dexp"
                                , "dincome"
                                , "iincome"
                                , "creditor"
                                , "debtor"
                            ]
                        }
                    },
                    {
                        "type": "Money",
                        "name": "amount",
                        "placeholder": "Amount",
                        "label": "Debit amount",
                        "format": "decimal",
                        "class": "x-debit-amount",
                        "ibukiEmitMessage": "DEBIT-CHANGED-FORWARDED",
                        "validations": [
                            {
                                "name": "required",
                                "message": "Amount is required"
                            }
                        ]
                    },
                    {
                        "type": "Text",
                        "name": "lineRefNo",
                        "placeholder": "Line ref",
                        "label": "Line ref",
                        "class": "x-line-ref",
                        "validations": []
                    },
                    {
                        "type": "Textarea",
                        "name": "remarks",
                        "class": "x-remarks",
                        "placeholder": "Line remarks",
                        "label": "Line remarks",
                        "validations": []
                    }
                ]
            }
        },
        {
            "type": "Range",
            "name": "credits",
            "label": "Credits",
            "layout": "table",
            "pattern": {
                "type": "Set",
                "name": "credit",
                "class": "x-range-set",
                "validations": [
                    {
                        "name": "gstPaymentVoucherValidation",
                        "message": "GST calculation error"
                    }
                ],
                "items": [
                    {
                        "type": "LedgerSubledger",
                        "name": "accId",
                        "placeholder": "Crebit account",
                        "label": "Crebit account",
                        "validations": [
                            {
                                "name": "required",
                                "message": "Ledger / subleder is required"
                            }
                        ],
                        "options": {
                            "args": [
                                "branch"
                                , "capital"
                                , "other"
                                , "loan"
                                , "iexp"
                                , "dexp"
                                , "dincome"
                                , "iincome"
                                , "creditor"
                                , "debtor"
                            ]
                        }
                    },
                    {
                        "type": "Money",
                        "name": "amount",
                        "placeholder": "Amount",
                        "label": "Credit amount",
                        "format": "decimal",
                        "class": "x-credit-amount",
                        "ibukiEmitMessage": "CREDIT-CHANGED-FORWARDED",
                        "validations": [
                            {
                                "name": "required",
                                "message": "Amount is required"
                            }
                        ]
                    },
                    {
                        "type": "Text",
                        "name": "lineRefNo",
                        "placeholder": "Line ref",
                        "class": "x-line-ref",
                        "label": "Line ref",
                        "validations": []
                    },
                    {
                        "type": "Textarea",
                        "name": "remarks",
                        "class": "x-remarks",
                        "placeholder": "Line remarks",
                        "label": "Line remarks",
                        "validations": []
                    }
                ]
            }
        },
        {
            "type": "Set",
            "name": "buttons",
            "class": "x-button-set",
            "items": [
                {
                    "type": "SubmitPrime",
                    "name": "submit",
                    "label": "Submit",
                    "class": "x-submit",
                    "methodName": "transformDataAndSubmit"
                },
                {
                    "type": "ButtonPrime",
                    "name": "reset",
                    "label": "Reset / Cancel",
                    "class": "x-button",
                    "methodName": "resetVoucher"
                }
            ]
        }
    ]
}